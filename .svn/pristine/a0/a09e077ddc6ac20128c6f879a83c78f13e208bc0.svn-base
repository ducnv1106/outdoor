<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>

        <import type="brite.outdoor.data.entities.ObjectSearch" />

        <variable
            name="mSearchViewModel"
            type="brite.outdoor.viewmodel.SearchViewModel" />
    </data>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:clickable="true"
    tools:context=".ui.fragments.search.FrmSearchVer2">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/clHeader"
        android:layout_marginTop="@dimen/_10sdp"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/tvTitle"
            app:layout_constraintTop_toTopOf="parent"
            android:text="@string/lblTitleSearch"
            android:textColor="@color/black"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10sdp"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/icFilter"
            android:background="@drawable/bg_filter_frm_search_ver2"
            app:layout_constraintEnd_toEndOf="parent"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/clHeader"
        android:id="@+id/clFilter">
        <LinearLayout
            android:id="@+id/clSearchUser"
            android:visibility="gone"
            android:paddingHorizontal="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_20sdp"
            android:layout_marginTop="@dimen/_25sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:orientation="horizontal"
            app:layout_constraintTop_toTopOf="parent"
            android:gravity="center_vertical"
            app:layout_constraintStart_toStartOf="parent"
            android:background="@drawable/bg_cl_search_user">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_smile"
                android:id="@+id/icClSearchUser"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lblSearchByPerson"
                android:textColor="@color/white"
                android:id="@+id/tvSearchUser"
                android:textSize="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_5sdp"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/clSearchLocation"
            android:visibility="gone"
            android:paddingHorizontal="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_20sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:orientation="horizontal"
            app:layout_constraintTop_toTopOf="@+id/clSearchUser"
            android:gravity="center_vertical"
            app:layout_constraintStart_toEndOf="@+id/clSearchUser"
            android:background="@drawable/bg_cl_search_location">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_map_pin"
                android:id="@+id/icClLocation"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lblSearchByLocation"
                android:textColor="@color/white"
                android:textSize="@dimen/_10sdp"
                android:id="@+id/tvSearchLocation"
                android:layout_marginStart="@dimen/_5sdp"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/clSearchTool"
            android:visibility="gone"
            android:paddingHorizontal="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_20sdp"
            android:layout_marginTop="@dimen/_15sdp"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/clSearchUser"
            android:gravity="center_vertical"
            app:layout_constraintStart_toStartOf="@+id/clSearchUser"
            android:background="@drawable/bg_cl_search_tool">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_vector"
                android:id="@+id/icClTool"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lblSearchByTool"
                android:textColor="@color/white"
                android:textSize="@dimen/_10sdp"
                android:id="@+id/tvSearchTool"
                android:layout_marginStart="@dimen/_5sdp"/>

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_marginTop="@dimen/_25sdp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_35sdp"
        android:background="@drawable/bg_edt_search"
        android:id="@+id/clEdtSearch"
        android:layout_marginHorizontal="@dimen/_10sdp"
        app:layout_constraintTop_toBottomOf="@+id/clFilter">
        <LinearLayout
            android:paddingHorizontal="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_20sdp"
            android:id="@+id/clSearchFollow"
            android:layout_marginStart="@dimen/_5sdp"
            android:orientation="horizontal"
            app:layout_constraintTop_toTopOf="parent"
            android:gravity="center_vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/bg_cl_search_location">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_map_pin"
                android:id="@+id/icClSearchFollow"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvSearchFollow"
                android:text="@string/lblSearchByLocation"
                android:textColor="@color/white"
                android:textSize="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_5sdp"/>

        </LinearLayout>

        <AutoCompleteTextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:id="@+id/edtSearch"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="@dimen/_12sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:hint="@string/str_hint_search"
            app:layout_constraintStart_toEndOf="@+id/clSearchFollow"
            app:layout_constraintEnd_toStartOf="@id/img_search"
            android:inputType="textFilter"
            android:imeOptions="actionSearch"
            android:singleLine="true"
            android:textColor="@android:color/black"
            android:layout_marginStart="@dimen/_5sdp"
            />
        <ImageView
            android:id="@+id/img_search"
            android:layout_marginEnd="@dimen/_5sdp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:src="@drawable/bg_ic_search_home_selector"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvNoData"
        android:visibility="gone"
        android:layout_marginTop="@dimen/_20sdp"
        android:text="@string/str_NoSearchData"
        app:layout_constraintTop_toTopOf="@+id/rvSearchWith"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <androidx.recyclerview.widget.RecyclerView
        android:layout_marginHorizontal="@dimen/_5sdp"
        android:id="@+id/rvSearchWith"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        android:layout_marginTop="@dimen/_19sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/clEdtSearch" />
    <com.facebook.shimmer.ShimmerFrameLayout
        android:layout_marginHorizontal="@dimen/_5sdp"
        android:id="@+id/shimmerFrameLayoutLocationOrUtensil"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:showOrHideShimmerLayout="@{mSearchViewModel.isShowHideShimmerLayoutLocationOrTool()}"
        android:layout_marginTop="@dimen/_19sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/clEdtSearch"
        android:layout_gravity="center"
        android:visibility="gone"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderLocationOrUtensil
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderLocationOrUtensil
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderLocationOrUtensil
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderLocationOrUtensil
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
    </com.facebook.shimmer.ShimmerFrameLayout>
    <com.facebook.shimmer.ShimmerFrameLayout
        android:layout_marginHorizontal="@dimen/_5sdp"
        android:id="@+id/shimmerFrameLayoutUser"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        android:layout_marginTop="@dimen/_19sdp"
        app:showOrHideShimmerLayout="@{mSearchViewModel.isShowHideShimmerLayoutUser()}"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/clEdtSearch"
        android:layout_gravity="center"
        android:visibility="gone"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderSearchUser
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderSearchUser
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderSearchUser
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderSearchUser
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderSearchUser
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderSearchUser
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderSearchUser
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <brite.outdoor.ui.widgets.CustomShimmerPlaceholderSearchUser
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
    </com.facebook.shimmer.ShimmerFrameLayout>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imgQuestion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_question"
        app:layout_constraintBottom_toTopOf="@id/glW8"
        app:layout_constraintEnd_toEndOf="parent" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glW8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.8" />
        <include
            android:id="@+id/naviBottomHome"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            layout="@layout/layout_bottom_navigation_home"/>

</androidx.constraintlayout.widget.ConstraintLayout>
</layout>