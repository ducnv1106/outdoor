<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rootView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@+id/ivBackGround"
        android:background="@drawable/bg_bottom_nav"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <FrameLayout
        android:id="@+id/frmLayout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clTabFooter"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true">
                <LinearLayout
                    android:id="@+id/layout_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:weightSum="5"
                    tools:ignore="MissingConstraints">

                    <LinearLayout
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">
                        <androidx.appcompat.widget.AppCompatRadioButton
                            android:button="@null"
                            android:id="@+id/btnHome"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_ic_home_selector" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">
                        <androidx.appcompat.widget.AppCompatRadioButton
                            android:button="@null"
                            android:id="@+id/btnSearch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_ic_search_home_selector" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">
                    </LinearLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <androidx.appcompat.widget.AppCompatRadioButton
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:gravity="center"
                            android:button="@null"
                            android:id="@+id/btnNotification"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_ic_notification_home_selector"/>
                        <TextView
                            android:gravity="center"
                            android:visibility="visible"
                            android:id="@+id/btnNotificationUnread"
                            app:layout_constraintTop_toTopOf="@id/btnNotification"
                            app:layout_constraintEnd_toEndOf="@id/btnNotification"
                            android:textColor="@color/colorWhite"
                            android:background="@drawable/bg_count_notification_unread"
                            android:layout_width="0dp"
                            android:layout_height="0dp"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <LinearLayout
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">
                        <androidx.appcompat.widget.AppCompatRadioButton
                            android:button="@null"
                            android:id="@+id/btnMyPage"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_ic_my_page_home_selector" />
                    </LinearLayout>
                </LinearLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
    <View
        android:id="@+id/vFakeButton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="1dp"
        android:layout_height="1dp"/>
    <androidx.appcompat.widget.AppCompatCheckBox
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/vFakeButton"
        app:layout_constraintTop_toTopOf="@+id/vFakeButton"
        android:button="@null"
        android:id="@+id/btnAddHome"
        android:background="@drawable/bg_ic_ad_home_selector"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <androidx.appcompat.widget.AppCompatImageButton
        android:layout_marginBottom="@dimen/_10sdp"
        android:visibility="gone"
        android:id="@+id/btnAddPlace"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="@dimen/_50sdp"
        app:layout_constraintBottom_toTopOf="@id/btnAddHome"
        android:src="@drawable/ic_place_home"
        android:background="@drawable/bg_item_place_utensils_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints" />
    <androidx.appcompat.widget.AppCompatImageButton
        android:layout_marginBottom="@dimen/_10sdp"
        android:visibility="gone"
        android:id="@+id/btnAddUtensils"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="@dimen/_50sdp"
        app:layout_constraintBottom_toTopOf="@id/btnAddHome"
        android:src="@drawable/ic_utensils_home"
        android:background="@drawable/bg_item_place_utensils_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>